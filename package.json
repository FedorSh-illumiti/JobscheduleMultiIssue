{
  "name": "gbicapmult2",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^8",
    "@sap/cds-mtxs": "^2",
    "express": "^4",
    "@sap/xssec": "^4",
    "@cap-js/hana": "^1"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.8.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8"
  },
  "scripts": {
    "start": "cds-serve",
    "undeploy": "cf undeploy gbicapmult2 --delete-services --delete-service-keys --delete-service-brokers"
  },
  "sapux": [
    "app/admin-books",
    "app/browse"
  ],
  "cds": {
    "profile": "with-mtx-sidecar",
    "requires": {
      "multitenancy": true,
      "auth": "xsuaa",
      "destinations": true,
      "jobscheduler": {
        "vcap": {
          "label": "jobscheduler"
        },
        "subscriptionDependency": "xsappname"
      }
    },
    "sql": {
      "native_hana_associations": false
    }
  }
}
